<!--[if IE 7]>
  <html class="no-js ie ie7 lt-ie9 lt-ie8" {{ site.language_attributes }}>
<![endif]-->

<!--[if IE 8]>
  <html class="no-js ie ie8 lt-ie9" {{ site.language_attributes }}>
